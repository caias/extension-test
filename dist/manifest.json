{
  "name": "Frontend TEAM Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "TEST",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://stash.wemakeprice.com/projects/WONDER/repos/prerender/pull-requests?create*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs",
    "cookies",
    "http://stash.wemakeprice.com/projects/WONDER/repos/prerender/*"
  ]
}