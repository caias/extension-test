{
  "name": "Frontend TEAM Extension",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "여러분의 시간을 아껴드립니다.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "slack.js"
      ],
      "matches": [
        "http://your-site/projects/WONDER/repos/prerender/pull-requests/*/*",
        "http://your-site/projects/TMFE/repos/*/pull-requests/*/*"
      ]
    },
    {
      "js": [
        "pullrequest.js"
      ],
      "matches": [
        "http://your-site/projects/WONDER/repos/prerender/pull-requests?create*",
        "http://your-site/projects/TMFE/repos/*/pull-requests?create*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs",
    "cookies",
    "storage",
    "http://your-site/projects/WONDER/repos/prerender/*",
    "http://your-site/projects/TMFE/repos/front-manager/*",
    "notifications"
  ]
}